<header>
    <div class='card pb-2'>
        {{#with navigation}}

        <nav class="nav">

            {{#each links}}
            <div class='nav-item'>
                <a href="{{href}}" class='nav-link'>{{text}}</a>
            </div>
            {{/each}}

            <div class="nav-item">
                <a href="{{href}}" class='nav-link disabled'>Disabled</a>
            </div>

        </nav>

        {{/with}}
    </div>
</header>