<div>
    {{#with web}}
    <div>
        <h1>WEB</h1>
    </div>
    <div>
        <h2>Languages</h2>
        <hr>
        <ol>
            {{#each languages}}
            <li>
                <hr>

                <div>Name: <b>{{name}}</b></div>
                <div>Version: <b>{{version}}</b></div>
                <div>
                    <p>Description: <b>{{description}}</b></p>
                </div>
                <br>
                <hr>
                {{#each elements}}
                <h3>Elements</h3>
                <ul>
                    <li>Name - {{name}}</li>
                    <div>
                        <p>Description - {{description}}</p>
                    </div>

                    <h4>Attributes</h4>
                    <ul>
                        {{#each attributes}}
                        <li>
                            <div>{{name}}</div>
                            <div>
                                <p>{{description}}</p>
                            </div>
                        </li>
                        {{/each}}
                    </ul>
                    <br>
                </ul>
                {{/each}}

                {{#each properties}}
                <h3>Properties</h3>
                <ul>
                    <li>Name - {{name}}</li>
                    <div>
                        <p>Description - {{description}}</p>
                    </div>
                    <h4>Values</h4>
                    <ul>
                        {{#each values}}
                        <li>
                            <div>{{value}}</div>
                            <div>
                                <p>{{description}}</p>
                            </div>
                        </li>
                        {{/each}}
                    </ul>
                    <br>
                </ul>
                <br>
                {{/each}}

                {{#each functions}}
                <h3>Functions</h3>
                <ul>
                    <li>Name - {{name}}</li>
                    <div>
                        <p>Description - {{description}}</p>
                    </div>

                    <h4>Parameters</h4>
                    <ul>
                        {{#each parameters}}
                        <li>
                            <div>{{name}}</div>
                            <div>{{type}}</div>
                            <div>
                                <p>{{description}}</p>
                            </div>
                        </li>
                        {{/each}}
                    </ul>
                    <br>
                </ul>
                {{/each}}

            </li>
            {{/each}}

        </ol>
    </div>
    {{/with}}
</div>