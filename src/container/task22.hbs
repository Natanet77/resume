<div>
    <div>
        {{#with page_header}}
        <h1 class="text-center">{{title}}</h1>
        <p class="text-center text-uppercase">{{description}}</p>
        {{/with}}
    </div>
    <br>
    <div>
        {{#with section}}
        {{#with about_us}}
        <div class="card">
            <div class="card-body">
                {{#with title_list}}
                <div class="display-6 text-end">{{main}}</div>
                <br>
                <h2 class="text-end">{{sub}}</h2>
                {{/with}}
                <br>
                {{#with button}}
                <div><button class="btn btn-outline-info">{{text}}</button></div>
                {{/with}}
            </div>
        </div>
        {{/with}}
        <br>
        <div class="card">
            <div class="card-body">

                {{#with advantage}}
                <div class="display-5">{{title}}</div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with efficiency}}
                        <div class="display-4 text-lowercase">{{title}}</div>
                        <br>
                        <h6>{{sub_title}}</h6>
                        <br>
                        <p>{{description}}</p>
                        <br>
                        <div><button class="btn btn-outline-warning">{{button}}</button></div>
                        {{/with}}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with innovation}}
                        <div class="display-4 text-lowercase text-center">{{title}}</div>
                        <br>
                        <h6 class="text-center">{{sub_title}}</h6>
                        <br>
                        <p class="text-center">{{description}}</p>
                        <br>
                        <div><button class="btn btn-outline-warning">{{button}}</button></div>
                        {{/with}}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with collaboration}}
                        <div class="display-4 text-lowercase text-end">{{title}}</div>
                        <br>
                        <h6 class="text-end">{{sub_title}}</h6>
                        <br>
                        <p class="text-end">{{description}}</p>
                        <br>
                        <div><button class="btn btn-outline-warning">{{button}}</button></div>
                        {{/with}}
                    </div>
                </div>
                {{/with}}
            </div>
        </div>
        <br>
        <div class="card">
            <div class="card-body">
                {{#with agreement}}
                <div class="display-5 text-lowercase">{{title}}</div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with info}}
                        <h5>{{sub_title}}</h5>
                        <br>
                        <div class="card">
                            <div class="card-body">
                                {{#with detail}}
                                <p>{{text}}</p>
                                <br>
                                <div><button class="btn btn-success">{{button}}</button></div>
                                {{/with}}
                            </div>
                        </div>
                        {{/with}}
                    </div>
                </div>
                <br>
                {{/with}}
            </div>
        </div>
        {{/with}}
    </div>